<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFAFC"
    tools:context=".startup">

    <Button
        android:id="@+id/primary"
        android:layout_width="354dp"
        android:layout_height="70dp"
        android:layout_marginBottom="416dp"
        android:backgroundTint="#61DA83"
        android:fontFamily="@font/roboto"
        android:text="Primary"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:cornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent" />


    <Button
        android:id="@+id/secondary"
        android:layout_width="354dp"
        android:layout_height="70dp"
        android:layout_marginBottom="328dp"
        android:backgroundTint="#8F99EB"
        android:fontFamily="@font/roboto"
        android:text="Secondary"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:cornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/primary"
        app:layout_constraintVertical_bias="0.833" />

    <Button
        android:id="@+id/JCPoly"
        android:layout_width="354dp"
        android:layout_height="70dp"
        android:layout_marginBottom="244dp"
        android:backgroundTint="#FFD316"
        android:fontFamily="@font/roboto"
        android:text="JC/Poly"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:cornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secondary"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/university"
        android:layout_width="354dp"
        android:layout_height="70dp"
        android:backgroundTint="#FE8484"
        android:fontFamily="@font/roboto"
        android:text="University"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:cornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/JCPoly"
        app:layout_constraintVertical_bias="0.097" />

    <Button
        android:id="@+id/skip"
        android:layout_width="354dp"
        android:layout_height="70dp"
        android:layout_marginBottom="44dp"
        android:backgroundTint="#030303"
        android:fontFamily="@font/roboto"
        android:text="Skip"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/university"
        app:layout_constraintVertical_bias="0.72" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="213dp"
        android:layout_height="57dp"
        android:fontFamily="@font/roboto"
        android:text="Hello There!"
        android:textColor="#000000"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.103" />

    <TextView
        android:id="@+id/what_is_you"
        android:layout_width="327dp"
        android:layout_height="64dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:fontFamily="@font/roboto"
        android:gravity="top"
        android:text="What is your current education level?"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />


</androidx.constraintlayout.widget.ConstraintLayout>